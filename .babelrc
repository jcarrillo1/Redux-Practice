{
  "presets": ["react", "es2015"],
  "env": {
   "development": {
      "plugins": [
        "typecheck",
        ["react-transform", {
            "transforms": [{
                "transform": "react-transform-hmr",
                "imports": ["react"],
                locals: [ "module" ]
              }
            ]
        }]
      ]
    }
  }
}